 <!-- Alternative Table -->
<div id="view-altr" class="container view-altr">
    <table class="table table-bordered table-hover">
        <thead class="thead-dark">
            <tr>
                <th colspan="2">Alternatif</th>
                <th colspan="5">Criteria</th>
            </tr>
        </thead>
        <thead class="thead-dark"> 
            <tr>
                <th>Kode</th>
                <th>Nama</th>
                <th>C1</th>
                <th>C2</th>
                <th>C3</th>
                <th>C4</th>
                <th>C5</th>
            </tr>
        </thead>
        <% if(alternative.length > 0){ %>
        <tbody>
            <% for(let altr of alternative){ %>
            <tr>
                <td><%= altr.id %> </td>
                <td> <%= altr.name %>   </td>
                <td> <%= altr.C1 %>  </td>
                <td> <%= altr.C2 %>  </td>
                <td> <%= altr.C3 %>  </td>
                <td> <%= altr.C4 %>  </td>
                <td> <%= altr.C5 %>  </td>
            </tr>
            <% } %>   
        </tbody>
        <% } else { %>
            <h2>There's no value of Alternatives</h2>
        <% } %>
    </table>
</div>